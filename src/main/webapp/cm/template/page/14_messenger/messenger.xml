    						<xf:group class="textbox" id="">
    							<w2:textbox class="name" id="" label="Team A" style="" tagname="span"></w2:textbox>
    							<w2:span class="badge" id="" label="5" style=""></w2:span>
    							<w2:textbox class="preview" id="" label="오늘 하루 고생하셨습니다." style="" tagname="div"></w2:textbox>
    						</xf:group>
    						<w2:span class="time" id="" label="09:00" style=""></w2:span>
    					</xf:group>
    					<xf:group class="" ev:onclick="scwin.chatOpen_onclick" id="" style="" tagname="li">
    						<xf:image alt="프로필사진" class="profile" id="" src="/cm/images/base/msg_profile1.png" style=""></xf:image>
    						<xf:group class="textbox" id="">
    							<w2:textbox class="name" id="" label="Team B" style="" tagname="span"></w2:textbox>
    							<w2:textbox class="preview" id="" label="오늘 하루 고생하셨습니다." style="" tagname="div"></w2:textbox>
    						</xf:group>
    						<w2:span class="time" id="" label="09:00" style=""></w2:span>
    					</xf:group>
    					<xf:group class="" ev:onclick="scwin.chatOpen_onclick" id="" style="" tagname="li">
    						<xf:image alt="프로필사진" class="profile" id="" src="/cm/images/base/msg_profile1.png" style=""></xf:image>
    						<xf:group class="textbox" id="">
    							<w2:textbox class="name" id="" label="Team C" style="" tagname="span"></w2:textbox>
    							<w2:textbox class="preview" id="" label="안녕하세요. 구매하신 소중한 상품이 배송완료 처리되었습니다." style=""
