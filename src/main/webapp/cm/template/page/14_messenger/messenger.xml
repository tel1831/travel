    			<xf:group class="chat_list" id="">
    				<xf:group class="titlebox" id="" style="">
    					<xf:trigger class="close" id="" style="" type="button">
    						<xf:label><![CDATA[Close]]></xf:label>
    					</xf:trigger>
    					<w2:textbox class="title" id="" label="Message" style="" tagname="span"></w2:textbox>
    				</xf:group>
    				<xf:group class="tab" id="" style="" tagname="ul">
    					<xf:group class="on" ev:onclick="scwin.msgTab_onclick" id="" tagname="li">
    						<w2:textbox id="" label="All" style="" tagname="span"></w2:textbox>
    						<w2:span class="badge" id="" label="5" style=""></w2:span>
    					</xf:group>
    					<xf:group class="" ev:onclick="scwin.msgTab_onclick" id="" style="" tagname="li">
    						<w2:textbox id="" label="Read" style="" tagname="span"></w2:textbox>
