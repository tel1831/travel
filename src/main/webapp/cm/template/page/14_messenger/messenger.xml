    						<w2:textbox id="" label="Unread" style="" tagname="span"></w2:textbox>
    					</xf:group>
    				</xf:group>
    				<xf:group class="list" id="" style="" tagname="ul">
    					<xf:group class="" ev:onclick="scwin.chatOpen_onclick" id="" tagname="li">
